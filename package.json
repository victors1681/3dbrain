{
  "name": "3dbrain",
  "description": "ThreeJS Brain Visualization",
  "version": "2.0.1",
  "main": "dist/amelia_brain.js",
  "keywords": [
    "amelia",
    "brain",
    "ai"
  ],
  "author": "Victor Santos",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --host 0.0.0.0 --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js src --fix",
    "build": "node build/build.js",
    "dist": "build/webpack.prod.conf.js",
    "test": "jest",
    "test:clean": "rimraf ./coverage",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage:report": "open ./coverage/lcov-report/index.html",
    "deploy": "npm run build && firebase deploy"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-glslify": "^2.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babelify": "^8.0.0",
    "browserify": "^14.3.0",
    "chalk": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glslify": "^6.1.1",
    "glslify-loader": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "ify-loader": "^1.0.4",
    "jest": "^23.1.0",
    "json-loader": "^0.5.4",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.8",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@superguigui/wagner": "^0.1.6",
    "dat-gui": "^0.5.0",
    "findup": "^0.1.5",
    "glsl-checker": "^1.0.1",
    "glsl-noise": "0.0.0",
    "glslify-fancy-imports": "^1.0.1",
    "glslify-hex": "^2.0.1",
    "gsap": "^1.20.4",
    "lodash": "^4.17.10",
    "postprocessing": "^4.5.0",
    "socket.io-client": "^2.1.1",
    "test": "^0.6.0",
    "three": "^0.91.0",
    "three-bas": "^2.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "glslify"
          ]
        }
      ],
      "glslify"
    ]
  },
  "glslify": {
    "transform": [
      "glslify-fancy-imports",
      "glslify-hex"
    ]
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
